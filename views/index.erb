<article>
  <div class="add-memo">
    <h1>memo app.</h1>
    <form action="/memos/new" method="get">
      <input type="submit" value="追加">
    </form>
  </div>
  <section>
    <ul>
      <% @memos.each do |memo| %>
        <li>
          <a href="/memos/<%= memo[:id] %>"><%= h memo[:title] %></a>
        </li>
      <% end %>
    </ul>
  </section>
</article>
